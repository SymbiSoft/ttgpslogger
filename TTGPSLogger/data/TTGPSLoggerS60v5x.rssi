#ifdef __S60_50__
// ---------------------------------------------------------------------------- 
// r_ttgp_toolbar_main
// ---------------------------------------------------------------------------- 
RESOURCE AVKON_TOOLBAR r_ttgp_toolbar_main
    {
    flags = KAknToolbarFixed;
    items =
        {
        TBAR_CTRL
            {
            type = EAknCtButton;
            id = ETTGPSLoggerTBMainLock;
            control = AVKON_BUTTON
                {
                flags = 0;
                states =
                    {
                    AVKON_BUTTON_STATE
                        {
                        txt = qtn_toolbar_main_lock;
                        helptxt = qtn_toolbarhelp_main_lock;
                        }  
                    };
                };
            },
        TBAR_CTRL
            {
            type = EAknCtButton;
            id = ETTGPSLoggerTBMainStartNow;
            control = AVKON_BUTTON
                {
                flags = 0;
                states =
                    {
                    AVKON_BUTTON_STATE
                        {
                        txt = qtn_toolbar_main_start;
                        helptxt = qtn_toolbarhelp_main_start;
                        }  
                    };
                };
            },
        TBAR_CTRL
            {
