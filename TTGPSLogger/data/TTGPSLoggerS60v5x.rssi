                        }  
                    };
                };
            },
        TBAR_CTRL
            {
            type = EAknCtButton;
            id = ETTGPSLoggerTBMainResume;
            control = AVKON_BUTTON
                {
                flags = 0;
                states =
                    {
                    AVKON_BUTTON_STATE
                        {
                        txt = qtn_toolbar_main_resume;
                        helptxt = qtn_toolbarhelp_main_resume;
                        }  
                    };
                };
            },
        TBAR_CTRL
            {
            type = EAknCtButton;
            id = ETTGPSLoggerTBMainStop;
            control = AVKON_BUTTON
                {
                flags = 0;
                states =
                    {
                    AVKON_BUTTON_STATE
                        {
                        txt = qtn_toolbar_main_stop;
                        helptxt = qtn_toolbarhelp_main_stop;
                        }  
